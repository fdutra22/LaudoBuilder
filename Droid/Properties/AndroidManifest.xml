				<action android:name="android.intent.action.ACTION_BOOT_COMPLETED" />
				<action android:name="com.google.android.c2dm.intent.RECEIVE" />
				<category android:name="com.example.gcm" />
			</intent-filter>
		</receiver>
	</application>
</manifest>